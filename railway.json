{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
        "dockerfilePath": "Laravel/Dockerfile",
        "watchPatterns": [
            "Laravel/**"
        ]
    },
    "deploy": {
        "startCommand": "sh start.sh",
        "healthcheckPath": "/api/test",
        "restartPolicyType": "ON_FAILURE"
    }
}